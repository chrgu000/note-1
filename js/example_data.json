n      edge [weight=20];\n      14 -> X4 -> 3;\n      3 -> X4 -> 14 [style=\"invis\"];\n      /* add a reverse path so graphviz doesn't force 14 above X4 above 3 */\n    }\n    {\n      edge [dir=\"both\", weight=20];\n      8 -> X3 -> 7;\n      7 -> X3 -> 8 [style=\"invis\"];\n      edge [style=\"invis\"];\n      X4 -> phantom6;\n      1 -> phantom2;\n      8 -> phantom2;\n    }\n}\n----\n\n\n== graphviz-15\n\n[graphviz, graphviz-15, svg]\n----\ndigraph\n{\n   rankdir=\"LR\";\n   node[shape=record, height=\"0.4\", width=\"0.4\"];\n   edge[dir=none];\n\n   E; // E is first node to appear\n   A -> B -> C;\n   D -> E -> F;\n   G -> H -> I;\n\n   edge[constraint=false]\n   A -> D -> G;\n\n   subgraph clusterX { A; B; }\n   subgraph clusterY { D;E; F; G;H; I; }\n}\n----\n\n\n= Erd\n== Erd 1\n\n[erd, erd-graphvi-1, svg]\n----\n#title {label: \"nfldb Entity-Relationship diagram (condensed)\", size: \"20\"}\n\n# Entities\n\n[F_V_SV_IMSI_H] {bgcolor: \"#d0e0d0\",size: \"12\"}\n[F_VOLTE_SIP_Mw_CITY_AREA_VD_IMSI_H] {bgcolor: \"#d0e0d0\",size: \"12\"}\n[F_V_BXDR_DROP_IMSI_H] {bgcolor: \"#d0e0d0\",size: \"12\"}\n[F_V_BXDR_VTV_CITY_AREA_VD_IMSI_H] {bgcolor: \"#d0e0d0\",size: \"12\"}\n[F_V_BXDR_IMSI_H] {bgcolor: \"#d0e0d0\",size: \"12\"}\n[F_V_BXDR_SRVCC_IMSI_H] {bgcolor: \"#d0e0d0\",size: \"12\"}\n\n[ALL] {bgcolor: \"#d0e0d0\"}\n\n\n# Relationships\n\nF_V_SV_IMSI_H      *--? ALL\nF_VOLTE_SIP_Mw_CITY_AREA_VD_IMSI_H      1--1 ALL\nF_V_BXDR_DROP_IMSI_H      1--1 ALL\nF_V_BXDR_VTV_CITY_AREA_VD_IMSI_H      1--1 ALL\nF_V_BXDR_IMSI_H      1--1 ALL\nF_V_BXDR_SRVCC_IMSI_H      1--1 ALL\n----\n",
