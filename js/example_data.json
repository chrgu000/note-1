        "content": "\ninclude::content/post/base.adoc[]\n:toc-title: graphviz\n\n[plantuml, graphviz-1, svg]\n----\ndigraph g {\n    subgraph mother\n    {\n        O [shape=record, label = \"{GitObject| ref (sha1)}\"];\n    }\n    subgraph herited\n    {\n        rankdir=LR;\n        rank=\"same\";\n        \"Tree\" [shape=box];\n        \"Tag\" [shape=box];\n        \"Blob\" [shape=box];\n        \"Commit\" [shape=box];\n    }\n    O -> \"Tree\" [arrowhead=\"onormal\", dir=back];\n    O -> \"Blob\" [arrowhead=\"onormal\", dir=back];\n    O -> \"Commit\" [arrowhead=\"onormal\", dir=back];\n    O -> \"Tag\" [arrowhead=\"onormal\", dir=back];\n    \"Tree\" -> \"Tree\" [arrowhead=\"vee\", label=\" 0..*\\n0..*1\"];\n    \"Tree\" -> \"Blob\" [arrowhead=\"vee\", label=\" 0..*\\n1\"];\n    \"Commit\" -> \"Tree\" [arrowhead=\"vee\", label=\" 1..*\\n1\"];\n    \"Tag\" -> \"Commit\" [arrowhead=\"vee\", label=\" 0..*\\n1\"];\n}\n----\n\n[plantuml, graphviz-2, svg]\n----\ndigraph g {\n  ranksep=0.2;\n  node[shape=box, width=2.3, height=0.6, fontname=\"Arial\"];\n  n1[label=\"Incident Commander\"];\n  n2[label=\"Public Information\\nOfficer\"];\n  n3[label=\"Liaison Officer\"];\n  n4[label=\"Safety Officer\"];\n  n5[label=\"Operations Section\"];\n  n6[label=\"Planning Section\"];\n  n7[label=\"Logistics Section\"];\n  n8[label=\"Finance/Admin. Section\"];\n  node[shape=none, width=0, height=0, label=\"\"];\n  edge[dir=none];\n  n1 -> p1 -> p2 -> p3;\n  {rank=same; n2 -> p1 ;}\n  {rank=same; p2 -> n4;}\n  {rank=same; p4 -> p5 -> p3 -> p6 -> p7;}\n  p4 -> n5;\n  p5 -> n6;\n  p6 -> n7;\n  p7 -> n8;\n}\n----\n\n\n[plantuml, graphviz-3, svg]\n----\ndigraph G {\n  a -> c;\n  a -> b;\n  b -> c [constraint=false];\n}\n----\n\n\n== graphviz-4\n\n[plantuml, graphviz-4, svg]\n----\ndigraph CentralPmr {\n  fontname=\"Helvetica\";\n  shape=box;\n  node[shape=box];\n  graph [splines=ortho]\n\n  sg  [label=\"TTD storage group for\\nthe logged values\"]\n  vc  [label=\"Value catalogue\"]\n  tc1 [label=\"Time catalogoue (1)\"]\n  tc2 [label=\"Time catalogoue (2)\"]\n  sv_ [shape=point,width=0.01,height=0.01];\n  sv  [label=\"\"]\n  ie  [shape=none, label=\"Initiating event\"]\n  c1  [shape=none, label=\"The set of values, defined\\nby the value catalogue, which\\nare freezed out of the TTD\\nstorage group of the actual log.\"]\n  c2  [shape=none, label=\"Time catalogue defining\\nat what time around the\\ninitiating event values\\nshould be collected.\"]\n  sgf [shape=record, label=\"{ 1|2|3|4|..}|{ | | | | }\"]\n\n  sg  -> sv_ [penwidth=4, dir=none];\n  sv_ -> sv -> tc2 [penwidth=4]\n  sv  -> sgf:f0 [penwidth=4]\n  {vc, tc1}  -> sg\n  c1  -> sv [style=dashed, arrowhead=\"open\"];\n\n  {rank=min;  ie}\n  {rank=same; sg c1}\n  {rank=same; vc sgf}\n  {rank=max;  rc2}\n}\n----\n\n\n\n== graphviz-5\n\n[plantuml, graphviz-5, svg]\n----\ndigraph CentralPmr2 {\n  fontname=\"Helvetica\";\n  shape=box;\n  node[shape=box];\n  graph [splines=ortho]\n\n  n1 -> n2 [headlabel=\"xxxx\"];\n  {rank=same; n2 -> n3 [label=\"yyyy\"]};\n  n3 -> n1;\n\n\n\n}\n----\n\n== graphviz-6\n\n[plantuml, graphviz-6, svg]\n----\ndigraph G {\n  rankdir=\"LR\";\n  node [fontname = \"font-awesome\"];\n  subgraph cluster3 {\n      4 -> 5 [color=\"#717070\",arrowsize=.5];\n      8 -> 4[color=\"#717070\",arrowsize=.5];\n\n      subgraph cluster31{\n        4 [fillcolor=\"#F5BDA2\", shape=rect;label=, style=\"filled\",fontsize=6,fixedsize=true,width=0.5,height=0.2,shape=Mrecord];\n        5 [fillcolor=\"#F5BDA2\", shape=ellipse;label=, style=\"filled\"];\n      }\n      subgraph cluster32{\n        8 [fillcolor=\"#F5BDA2\", shape=rect;label=, style=\"filled\",shape=box];\n        6 [fillcolor=\"#F5BDA2\", shape=ellipse;label=, style=\"filled\"];\n      }\n  }\n}\n----\n\n\n== graphviz-7\n\n\n\n== 3.17.53-1\n\n[plantuml, 3.17.53-1, svg]\n----\ndigraph G {\n  graph [splines=ortho]\n\n  node[shape=oval,width=0.2, height=0.2,fillcolor=cornflowerblue,fontname=\"Arial\"];\n  start[label=\"Toolbox Start\"];\n\n  node[shape=box, width=2.0, height=0.4,fontname=\"Microsoft Yahei\"];\n  md[label=\"MessageDriver\"];\n  md1[label=\"MessageDriver\"];\n  t[label=\"Topic\"];\n  cs[label=\"CallbackServlet\"];\n  a[label=\"Activator\"];\n  a2[label=\"Activator\"];\n  main[label=\"Main\"];\n  ml[label=\"MessageListener\"];\n  pth[label=\"PlanTaskHelper\"];\n\n  node[shape=box3d, width=2.0, height=0.4];\n  q1[label=\"MQ (Public)\"];\n  q2[label=\"MQ (Private)\"];\n\n\n  node[shape=plaintext, fontsize=11,group=a];\n  pt1[label=\"Activator#start()\"];\n  pt2[label=\"MessageDriver#listen(topic)\"];\n  pt3[label=\"new URL(url).getContent()\"];\n  pt4[label=\"MessageDriver.trigger(json)\"];\n  pt5[label=\"MessageDriver.onmessage(json)\"];\n  pt6[label=\"top.handler.on(window)\"];\n  pt7[label=\"MessageDriver.within()\"];\n  pt8[label=\"MessageDriver.check()\"];\n  pt9[label=\"PlanTaskHelper.executeMessageProgram()\"];\n\n  subgraph clusterset1 {\n    start -> a;\n    a -> pt1;\n    pt1 -> md;\n    md -> pt2;\n\n    label = \"\";\n    color=green;\n  };\n\n  subgraph clusterset2 {\n    {rank=same;main -> pt3;};\n    {rank=same;pt3 -> cs;};\n    {rank=same;cs -> pt4;};\n    {pt4 -> q1;};\n\n    {rank=same;pt5 -> q1[dir=back]};\n\n    {rank=same;md1 -> pt5[dir=back]};\n    {rank=same;pt6 -> md1[dir=back]};\n    {pt6 -> pth};\n    {pth -> pt7};\n    {pt7 -> pt8};\n    {pt8 -> pt9};\n    {rank=same;pth;pt7;pt8;pt9};\n    label = \"\";\n    color=green;\n  };\n}\n\n----\n\n\n\n\n[plantuml, 3.17.53-1-1, svg]\n----\ndigraph G {\n  graph [splines=ortho]\n\n  node[shape=oval,width=0.2, height=0.2,fillcolor=cornflowerblue,fontname=\"Arial\"];\n  start[label=\"Toolbox Start\"];\n\n  node[shape=box, width=2.0, height=0.4,fontname=\"Microsoft Yahei\"];\n  md[label=\"MessageDriver\"];\n  a[label=\"Activator\"];\n\n  node[shape=plaintext, fontsize=11,group=a];\n  pt1[label=\"Activator#start()\"];\n  pt2[label=\"MessageDriver#listen(topic)\"];\n\n  subgraph clusterset1 {\n    start -> a;\n    a -> pt1;\n    pt1 -> md;\n    md -> pt2;\n    {rank=same;start;a;pt1;md;pt2;};\n\n    label = \"\";\n    color=green;\n  };\n\n}\n----\n\n[plantuml, 3.17.53-1-2, svg]\n----\ndigraph G {\n  graph [splines=ortho]\n\n\n  node[shape=box, width=2.0, height=0.4,fontname=\"Microsoft Yahei\"];\n  md1[label=\"MessageDriver\"];\n  cs[label=\"CallbackServlet\"];\n  main[label=\"Main\"];\n  pth[label=\"PlanTaskHelper\"];\n\n  node[shape=box3d, width=2.0, height=0.4];\n  q1[label=\"MQ (Public)\"];\n\n\n  node[shape=plaintext, fontsize=11,group=a];\n  pt3[label=\"new URL(url).getContent()\"];\n  pt4[label=\"MessageDriver.trigger(json)\"];\n  pt5[label=\"MessageDriver.onmessage(json)\"];\n  pt6[label=\"top.handler.on(window)\"];\n  pt7[label=\"MessageDriver.within()\"];\n  pt8[label=\"MessageDriver.check()\"];\n  pt9[label=\"PlanTaskHelper.executeMessageProgram()\"];\n\n  subgraph clusterset2 {\n    {rank=same;main -> pt3;};\n    {rank=same;pt3 -> cs;};\n    {rank=same;cs -> pt4;};\n    {pt4 -> q1;};\n\n    {rank=same;pt5 -> q1[dir=back]};\n\n    {rank=same;md1 -> pt5[dir=back]};\n    {rank=same;pt6 -> md1[dir=back]};\n    {pt6 -> pth};\n    {pth -> pt7};\n    {pt7 -> pt8};\n    {pt8 -> pt9};\n    {rank=same;pth;pt7;pt8;pt9};\n    label = \"\";\n    color=green;\n  };\n}\n\n----\n\n\n== 监控 hdfs 文件,触发消息\n\n[plantuml, 3.17.53-2, svg]\n----\ndigraph G {\n  graph [splines=ortho]\n\n  node[shape=box, width=2.0, height=0.4, fontname=\"Arial\"];\n  a[label=\"Activator\"];\n  md[label=\"MessageDriver\"];\n  ml[label=\"MessageListener\"];\n  s[label=\"Spark\"];\n\n  node[shape=box3d, width=3.0, height=0.4, fontname=\"Arial\"];\n  q1[label=\"MQ\"];\n\n  node[shape=plaintext, width=3.0, height=0.4, fontname=\"Arial\"];\n\n  node[shape=parallelogram, width=1.8, height=0.3, fontname=\"Arial\"];\n  p1[label=\"HDFSUtil.check\",fontsize=12.0,fixedsize=true];\n\n  node[shape=none, width=0, height=0, label=\"\"];\n  n1;\n  n2;\n\n  node[shape=point, width=0.1, height=0.1,label=\"\",style=filled, fillcolor=darkorange];\n  m1;\n\n  node[shape=oval,width=0.2, height=0.2,fillcolor=cornflowerblue];\n  start[label=\"Toolbox Start\"];\n  end[label=\"End\"];\n\n  start -> a [label=\"Activator#start\"];\n  a -> m1 [headlabel=\"MessageDriver#listen\",arrowhead=None];\n  {m1 -> md};\n\n  {rank=same;m1 -> p1 [arrowhead=None]};\n\n  {rank=same;p1 -> q1 [label=\"sendMessage()\"]};\n\n  {q1 -> ml [label=\"onMessage()\",dir=back]};\n\n  {rank=same;md -> ml [label=\"MessageDriver#onmessage()\",dir=back]};\n\n  {md -> s -> end}\n\n}\n\n----\n\n\n\n\n== graphviz-8\n\n\n\n== graphviz-9\n\n[plantuml, graphviz-9, svg]\n----\ndigraph G {\n  layout=\"neato\"\n  1[label=\"1\", pos=\"19.9759091241034,0.98135348654836!\", shape = \"square\"];\n  2[label=\"2\", pos=\"19.9036945334439,1.96034280659121!\", shape = \"square\"];\n  3[label=\"3\", pos=\"19.7835301992956,2.93460948910723!\", shape = \"square\"];\n  4[label=\"4\", pos=\"19.6157056080646,3.90180644032256!\", shape = \"square\"];\n  5[label=\"5\", pos=\"19.4006250638909,4.85960359806528!\", shape = \"square\"];\n  6[label=\"6\", pos=\"19.1388067146442,5.80569354508925!\", shape = \"square\"];\n  7[label=\"7\", pos=\"18.8308813036604,6.7377970678444!\", shape = \"square\"];\n  8[label=\"8\", pos=\"18.4775906502257,7.6536686473018!\", shape = \"square\"];\n  9[label=\"9\", pos=\"18.0797858624689,8.55110186860564!\", shape = \"square\"];\n  10[label=\"10\", pos=\"17.6384252869671,9.42793473651995!\", shape = \"square\"];\n  11[label=\"11\", pos=\"17.1545722000054,10.2820548838644!\", shape = \"square\"];\n  12[label=\"12\", pos=\"16.6293922460509,11.111404660392!\", shape = \"square\"];\n  13[label=\"13\", pos=\"16.0641506296129,11.9139860898487!\", shape = \"square\"];\n  14[label=\"14\", pos=\"15.4602090672547,12.6878656832729!\", shape = \"square\"];\n  15[label=\"15\", pos=\"14.8190225070992,13.4311790969404!\", shape = \"square\"];\n  16[label=\"16\", pos=\"14.142135623731,14.1421356237309!\", shape = \"square\"];\n  17[label=\