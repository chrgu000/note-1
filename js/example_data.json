ubgraph clusterset2 {\n    {rank=same;bjs -> b1 -> b2 -> b3 -> b4 -> b5};\n    label = ;\n    color=green;\n  }\n\n  newrank=true;\n  {rank=same;a2 -> bjs};\n\n  node[shape=box, width=2.0, height=0.4, fontname=\"Arial\",group=c];\n  jg[label=\"JobGenerator\"];\n  #jge[label=\"EventLoop[JobGeneratorEvent]\"];\n\n  node[shape=plaintext, fontsize=11];\n  c1[label=\"eventLoop.start()#78\"];\n  c2[label=\"startFirstTime()#100\"];\n  c3[label=\"graph.start()#194\"];\n  c4[label=\"timer.start()#195\"];\n\n  b4 -> jg -> c1 -> c2 -> c3 -> c4;\n\n  subgraph clusterset3 {\n    jg;c1;c2;c3;c4;\n    label = ;\n    color=green;\n  }\n\n  node[shape=box, width=2.0, height=0.4, fontname=\"Arial\",group=d];\n  rt[label=\"RecurringTimer\"];\n  jg1[label=\"JobGenerator\",tooltip =\"调用Callback\"];\n\n  node[shape=plaintext, fontsize=11];\n  d1[label=\"loop()#103\"];\n  d2[label=\"triggerActionForNextInterval()#92\"];\n\n  subgraph clusterset5 {\n    #newrank=true;\n    {rank=same;rt -> d1 -> d2 -> jg1};\n    label = ;\n    color=green;\n  }\n\n  newrank=true;\n  {rank=same;c4 -> rt};\n\n  subgraph clusterset7 {\n    node[shape=plaintext, fontsize=11];\n    e1 [label=\"callback()#61\",tooltip =\"JobGenerator#private val timer = new RecurringTimer(clock, ssc.graph.batchDuration.milliseconds,longTime => eventLoop.post(GenerateJobs(new Time(longTime))), \\\"JobGenerator\\\") \"];\n    e2 [label=\"processEvent(event)#89\",tooltip =\"JobGenerator#eventLoop#processEvent(event)\"];\n    e3 [label=\"generateJobs(time)#183\",tooltip =\"case GenerateJobs(time) => generateJobs(time)\"];\n    e4 [label=\"allocateBlocksToBatch(time)#248\",tooltip =\"jobScheduler.receiverTracker.allocateBlocksToBatch(time) // allocate received blocks to batch\"];\n    e5[label=\"generateJobs(time)\",tooltip =\"JobGenerator#generateJobs#243\"];\n\n    subgraph clusterset4 {\n      #{rank=same;jg -> e1[constraint=false]};\n      {rank=same;e1 -> e2 -> e3 -> e4};\n      label = ;\n      color=blue;\n    }\n\n    node[shape=box, width=2.0, height=0.4, fontname=\"Arial\",group=f];\n    fdg[label=\"DStreamGraph\"];\n    fds[label=\"ForEachDStream\"];\n\n    node[shape=plaintext, fontsize=11];\n    f1[label=\"generateJobs()#118\"];\n    f2[label=\"generateJob()#431\",tooltip =\"ForEachDStream#generateJob()\"];\n    f3[label=\"getOrCompute(time)#328\",tooltip =\"Get the RDD corresponding to the given time; either retrieve it from cache or compute-and-cache it.\"];\n    f4[label=\"RDD\",tooltip=\"Some(new Job(time, jobFunc))()#438\"];\n    f5[label=\"submitJobSet\",tooltip=\"jobScheduler.submitJobSet(JobSet(time, jobs, streamIdToInputInfos))#253\"];\n\n\n    subgraph clusterset6 {\n      {rank=same;fdg -> f1 -> fds -> f2 -> f3 -> f4};\n      label = ;\n      color=blue;\n    }\n\n    newrank=true;\n    {rank=same;c3; e1};\n    {rank=same;e5; fdg};\n    {rank=same;e5;jg};\n    #jg -> fdg [constraint=false];\n    e5 -> fdg;\n    e5 -> e1;\n    jg -> e5;\n\n    f4 -> f5;\n\n    label = ;\n    color=blue;\n  }\n\n  node[shape=box, width=2.0, height=0.4, fontname=\"Arial\",group=h];\n  hjs[label=\"JobScheduler\"];\n\n  node[shape=plaintext, fontsize=11];\n  h1[label=\"generateJobs()#118\"];\n  h2[label=\"jobExecutor.execute()#153\",tooltipo=\"jobSet.jobs.foreach(job => jobExecutor.execute(new JobHandler(job)))\"];\n\n\n  subgraph clusterset8 {\n    {hjs -> h1 -> h2};\n    label = ;\n    color=red;\n  }\n\n  newrank=true;\n  {rank=same;f5; hjs};\n  f5 -> hjs;\n}\n\n----\n",
